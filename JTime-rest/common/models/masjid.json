{
  "name": "Masjid",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": "true",
      "length": 50,
      "postgres": {
        "dataLength": 50
      }
    },
    "location": {
      "type": "GeoPoint",
      "required": true
    },
    "humanReadableAddress": {
      "type": "string",
      "required": false,
      "length": 100,
      "postgres": {
        "dataLength": 100
      }
    },
    "createdAt": {
      "type": "date",
      "required": "true",
      "defaultFn": "now"
    },
    "lastModifiedAt": {
      "type": "date",
      "required": "true",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "salaahtimes": {
      "type": "hasMany",
      "model": "SalaahTime",
      "foreignKey": ""
    }
  },
  "acls": [{
    "accessType": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  }, {
    "accessType": "READ",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "ALLOW",
    "property": ["find", "getTodayTimes", "getTimes"]
  }, {
    "accessType": "WRITE",
    "principalType": "ROLE",
    "principalId": "$authenticated",
    "permission": "ALLOW",
    "property": "create"
  }],
  "methods": {}
}
